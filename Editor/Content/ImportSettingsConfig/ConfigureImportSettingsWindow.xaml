<Window x:Class="Editor.Content.ConfigureImportSettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Editor.Content"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type={x:Type local:ConfigureImportSettings}}"
        Title="Configure Import Settings"
        WindowStartupLocation="CenterOwner"
        UseLayoutRounding="True"
        Height="800"
        Width="12800">
    <Grid>
        <TabControl Background="Transparent"
                    BorderThickness="0">
            <TabItem Header="Geometry" MinWidth="96"/>
            <TabItem Header="Texture" MinWidth="96"/>
            <TabItem Header="Audio" MinWidth="96"/>
        </TabControl>
    </Grid>
</Window>
