<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Editor.Background1">#0A0C10</Color>
    <Color x:Key="Editor.Background2">#242D37</Color>
    <Color x:Key="Editor.Background3">#364255</Color>
    <Color x:Key="Editor.Background4">#49596C</Color>
    <Color x:Key="Editor.Background5">#5E7088</Color>
    <Color x:Key="Editor.Font">#F9F6EE</Color>
    <Color x:Key="Editor.DisabledFont">#717171</Color>
    <Color x:Key="Editor.Select">#01579B</Color>
    <Color x:Key="Editor.Lightning">#FFC800</Color>
    <Color x:Key="Editor.Red">#F44336</Color>
    <Color x:Key="Editor.Green">#4CAF50</Color>
    <Color x:Key="Editor.Blue">#2196f3</Color>
    <Color x:Key="Editor.Orange">#FF5722</Color>
    <Color x:Key="Editor.Secret">#FE46A5</Color>
    <SolidColorBrush Color="{StaticResource Editor.Background1}" x:Key="Editor.BackgroundBrush1"/>
    <SolidColorBrush Color="{StaticResource Editor.Background2}" x:Key="Editor.BackgroundBrush2"/>
    <SolidColorBrush Color="{StaticResource Editor.Background3}" x:Key="Editor.BackgroundBrush3"/>
    <SolidColorBrush Color="{StaticResource Editor.Background4}" x:Key="Editor.BackgroundBrush4"/>
    <SolidColorBrush Color="{StaticResource Editor.Background5}" x:Key="Editor.BackgroundBrush5"/>
    <SolidColorBrush Color="{StaticResource Editor.Select}" x:Key="Editor.SelectBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Font}" x:Key="Editor.FontBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.DisabledFont}" x:Key="Editor.DisabledFontBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Lightning}" x:Key="Editor.LightningBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Red}" x:Key="Editor.RedBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Green}" x:Key="Editor.GreenBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Blue}" x:Key="Editor.BlueBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Orange}" x:Key="Editor.OrangeBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Secret}" x:Key="Editor.SecretBrush"/>
</ResourceDictionary>